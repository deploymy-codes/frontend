(function() {
  var AuthenticationConfiguration = function($authProvider) {
    $authProvider.tokenName     = 'api_key';
    $authProvider.tokenPrefix   = 'deploy_my_codes';
    $authProvider.github({
      clientId: '<%= settings.github.client_id %>',
      url:      '<%= settings.github.api_authentication_url %>'
    });
  };

  angular.module('DeployMyCodes').config(['$authProvider', AuthenticationConfiguration]);
})();
