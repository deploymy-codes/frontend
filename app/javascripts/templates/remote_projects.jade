section(class="block-list", ng-controller="RemoteProjectsCtl")
  header
    | remote projects

  ul(class="m-remote-project", ng-repeat="project in projects")
    li
      span
        | {{:: project.name }}

      div(class="switch")
        input(type="checkbox", id="{{:: project.name }}", ng-click="import(project)")
        label(for="{{:: project.name }}")
