require.register('deploy_my_codes/repositories/organization_repository', function(exports, require, module) {
  module.exports = function($http, $q, APIHelper) {
    var listOrganizations = function() {
      var deferred = $q.defer();

      $http.get(APIHelper.buildURL('/users/orgs'))
        .success(function(data, _, _, _) { deferred.resolve(data); });

      return deferred.promise;
    };

    return {
      listOrganizations: listOrganizations
    }
  };
});
